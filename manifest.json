{
  "manifest_version": 2,
  "name": "anti-bully",
  "version": "1.0",
  "description": "an extension to prevent online bullying",
  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "js": ["jquery-3.2.0.min.js", "twitterthing.js"],
      "run_at": "document_end"
    }
  ],
  "background":{
    "scripts" : ["jquery-3.2.0.min.js"]
  },
  "permissions":[
      "tabs",
      "http://www.twitter.com/*"

  ]
}
