{
  "manifest_version": 2,
  "name": "anti-bully",
  "version": "1.0",
  "description": "an extension to prevent online bullying",
  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "js": ["twitterthing.js"],
      "run_at": "document_end"
    }
  ],
  //"background":{
  //    "scripts":["background.js"]
  //},
  "permissions":[
      "tabs",
      "http://www.twitter.com/*"
  ]
}
